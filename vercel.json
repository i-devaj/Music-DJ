{
    "version": 2,
    "buildCommand": "cd client && npm install && npm run build",
    "outputDirectory": "client/dist",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://YOUR-RAILWAY-APP-URL/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }